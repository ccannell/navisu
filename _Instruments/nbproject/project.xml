<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.navisu.lib.instruments</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>org.navisu.lib.comm</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.navisu.lib.geodesy</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.navisu.lib.javafx</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.navisu.lib.nmea</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <friend-packages>
                <friend>org.navisu.nmea.devices</friend>
                <package>com.javafx.main</package>
                <package>org.navisu.instrument.controller</package>
                <package>org.navisu.instrument.controller.events</package>
                <package>org.navisu.instrument.controller.simulator</package>
                <package>org.navisu.instrument.misc</package>
                <package>org.navisu.instrument.model</package>
                <package>org.navisu.instrument.view.compass</package>
                <package>org.navisu.instrument.view.gps</package>
                <package>org.navisu.instrument.view.sounder</package>
                <package>org.navisu.widget.controller</package>
                <package>org.navisu.widget.model</package>
                <package>org.navisu.widget.view.buoy</package>
                <package>org.navisu.widget.view.button</package>
                <package>org.navisu.widget.view.clock</package>
                <package>org.navisu.widget.view.cube3D</package>
                <package>org.navisu.widget.view.googleWheater</package>
            </friend-packages>
            <class-path-extension>
                <runtime-relative-path>ext/instruments.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/instruments.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
